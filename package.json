{
  "name": "@goldentree1/metocean",
  "version": "0.1.5",
  "description": "Unofficial JS/TS wrapper for MetOcean Solutions' API, featuring type-safety and error-handling.",
  "author": "EB",
  "homepage": "https://github.com/goldentree101/node-metocean",
  "repository": {
    "type": "git",
    "url": "https://github.com/goldentree101/node-metocean"
  },
  "license": "MIT",
  "keywords": [
    "api",
    "MetOcean API",
    "MetOcean Solutions",
    "surfing",
    "ocean",
    "met",
    "ocean",
    "metocean",
    "marine",
    "marine weather",
    "accurate",
    "report",
    "weather",
    "surfing",
    "swell forecast",
    "surf",
    "surf forecast",
    "forecast",
    "wave",
    "waves",
    "wind"
  ],
  "main": "dist/cjs/metocean.js",
  "module": "dist/esm/metocean.js",
  "browser": "dist/browser/metocean.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prebuild:types": "rimraf ./build",
    "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "predocs": "rimraf ./docs",
    "docs": "typedoc src --out docs",
    "clean": "rimraf ./build ./dist ./docs",
    "gen": "rimraf ./build ./dist ./docs && rollup -c && tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run && typedoc src --out docs",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@microsoft/api-extractor": "^7.19.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^18.15.11",
    "babel-jest": "^29.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  }
}
